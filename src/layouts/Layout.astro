---
const { title } = Astro.props;

import Navbar from "../components/Navbar.svelte";

const description = "Kerala Startup Mission is the central agency of the Government of Kerala for entrepreneurship development and incubation activities in Kerala, India.";
const url = "https://startupmission.kerala.gov.in/";

---

<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- favicon -->
    <link rel="icon" type="image/png" href="favicon.png">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="assets/vendors/bootstrap/css/bootstrap.min.css" media="all">
    <!-- Fonts Awesome CSS -->
    <link rel="stylesheet" type="text/css" href="assets/vendors/fontawesome/css/all.min.css">
    <!-- jquery-ui css -->
    <link rel="stylesheet" type="text/css" href="assets/vendors/jquery-ui/jquery-ui.min.css">
    <!-- modal video css -->
    <link rel="stylesheet" type="text/css" href="assets/vendors/modal-video/modal-video.min.css">
    <!-- light box css -->
    <link rel="stylesheet" type="text/css" href="assets/vendors/lightbox/dist/css/lightbox.min.css">
    <!-- slick slider css -->
    <link rel="stylesheet" type="text/css" href="assets/vendors/slick/slick.css">
    <link rel="stylesheet" type="text/css" href="assets/vendors/slick/slick-theme.css">
    <!-- google fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,400&family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,400&display=swap" rel="stylesheet">
    <!-- Custom CSS -->
    <link rel="stylesheet" type="text/css" href="style.css">
    <title>Kerala Startup Mission | Cannovation 3.0 </title>
 </head>
 <body class="home">
    <Navbar client:load></Navbar>
		<slot />
    

	  <!-- JavaScript -->
    <script src="assets/js/jquery.js" is:inline></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js" is:inline></script>
    <script src="assets/vendors/bootstrap/js/bootstrap.min.js" is:inline></script>
    <script src="assets/vendors/jquery-ui/jquery-ui.min.js" is:inline></script>
    <script src="assets/vendors/countdown-date-loop-counter/loopcounter.js" is:inline></script>
    <script src="assets/js/jquery.counterup.js" is:inline></script>
    <script src="assets/vendors/modal-video/jquery-modal-video.min.js" is:inline></script>
    <script src="assets/vendors/masonry/masonry.pkgd.min.js" is:inline></script>
    <script src="assets/vendors/lightbox/dist/js/lightbox.min.js" is:inline></script>
    <script src="assets/vendors/slick/slick.min.js" is:inline></script>
    <script src="assets/js/jquery.slicknav.js" is:inline></script>
    <script src="assets/js/custom.min.js" is:inline></script>
 </body>
</html>